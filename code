print("Enter the number of jobs")
n=int(input())
print("Enter job start time, end time and earnings")
lst=[]
for i in range(n):
    start=int(input())
    end=int(input())
    earnings=int(input())
    lst.append(earnings)
    
print("The number of tasks and earnings available for others")
print("Task:",(n-1))
lst.sort()

sum=lst[0]+lst[1]

print("Earnings: ", sum)
